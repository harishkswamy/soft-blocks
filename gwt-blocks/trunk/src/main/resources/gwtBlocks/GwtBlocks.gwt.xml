<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
	<inherits name='jeeBlocks.JeeBlocks'/>

    <source path="client" />
    <source path="shared" />
    
    <stylesheet src="gwtBlocks/GwtBlocks.css" />

    <generate-with class="gwtBlocks.generators.BeanBindingModelGenerator">
        <when-type-assignable class="gwtBlocks.shared.models.BeanBindingModel" />
    </generate-with>
  
</module>
